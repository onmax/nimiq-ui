{
  "name": "nimiq-vitepress-theme",
  "type": "module",
  "version": "1.0.0-beta.5",
  "packageManager": "pnpm@10.2.1",
  "description": "Nimiq UI theme for VitePress",
  "author": "onmax",
  "license": "MIT",
  "homepage": "https://github.com/onmax/nimiq-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nimiq-ui.git"
  },
  "bugs": "https://github.com/onmax/nimiq-ui/issues",
  "keywords": [
    "nimiq",
    "nimiq-ui",
    "nimiq-theme",
    "vitepress",
    "vitepress-theme"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/assets/index.css",
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "vue-tsc --noEmit",
    "release": "pnpm run build && bumpp --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "vitepress": "^1.5.0 || ^2.0.0-alpha.3"
  },
  "dependencies": {
    "nimiq-icons": "workspace:*",
    "@iconify-json/tabler": "^1.2.16",
    "@nolebase/vitepress-plugin-git-changelog": "catalog:",
    "@vueuse/core": "^12.7.0",
    "mark.js": "^8.11.1",
    "minisearch": "^7.1.2",
    "p-map": "^7.0.3",
    "reka-ui": "^2.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify/utils": "^2.1.25",
    "@types/cross-spawn": "^6.0.6",
    "@unocss/cli": "^66.0.0",
    "@unocss/reset": "catalog:",
    "bumpp": "^10.0.3",
    "cross-spawn": "^7.0.6",
    "nimiq-css": "workspace:*",
    "pathe": "catalog:",
    "typescript": "catalog:",
    "unbuild": "^3.3.1",
    "unocss": "catalog:",
    "unocss-preset-onmax": "catalog:",
    "unplugin-vue-components": "^28.4.0"
  }
}
