{
  "name": "nimiq-vitepress-theme",
  "type": "module",
  "version": "1.0.0-beta.5",
  "packageManager": "pnpm@10.2.1",
  "description": "Nimiq UI theme for VitePress",
  "author": "onmax",
  "license": "MIT",
  "homepage": "https://github.com/onmax/nimiq-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nimiq-ui.git"
  },
  "bugs": "https://github.com/onmax/nimiq-ui/issues",
  "keywords": [
    "nimiq",
    "nimiq-ui",
    "nimiq-theme",
    "vitepress",
    "vitepress-theme"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/assets/index.css",
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "vue-tsc --noEmit",
    "release": "pnpm run build && bumpp --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "vitepress": "^1.5.0 || ^2.0.0-alpha.3"
  },
  "dependencies": {
    "@iconify-json/tabler": "catalog:",
    "@nolebase/vitepress-plugin-git-changelog": "catalog:",
    "@vueuse/core": "catalog:",
    "mark.js": "catalog:",
    "minisearch": "catalog:",
    "nimiq-icons": "workspace:*",
    "p-map": "catalog:",
    "reka-ui": "catalog:",
    "vitepress-plugin-group-icons": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@iconify/utils": "catalog:",
    "@shikijs/vitepress-twoslash": "catalog:",
    "@types/cross-spawn": "catalog:",
    "@unocss/cli": "catalog:",
    "@unocss/reset": "catalog:",
    "bumpp": "catalog:",
    "cross-spawn": "catalog:",
    "defu": "catalog:",
    "nimiq-css": "workspace:*",
    "pathe": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "unocss-preset-animations": "catalog:",
    "unocss-preset-onmax": "catalog:",
    "unplugin-vue-components": "catalog:"
  }
}
