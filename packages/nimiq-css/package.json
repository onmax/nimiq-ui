{
  "name": "nimiq-css",
  "type": "module",
  "version": "1.0.0-beta.19",
  "description": "Nimiq's Design System in CSS",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./css": {
      "import": "./dist/css/*"
    },
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@iconify/utils": "catalog:",
    "@unocss/preset-web-fonts": "catalog:",
    "@unocss/reset": "catalog:",
    "nimiq-icons": "workspace:*",
    "ts-cssjson": "catalog:",
    "unocss": "catalog:"
  },
  "devDependencies": {
    "@iconify/tools": "catalog:",
    "@iconify/types": "catalog:",
    "pathe": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
}
