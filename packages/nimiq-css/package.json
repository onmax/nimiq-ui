{
  "name": "nimiq-css",
  "type": "module",
  "version": "0.2.98",
  "description": "Nimiq's Design System in CSS",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    },
    "./css": {
      "import": "./dist/css/*"
    },
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "release": "pnpm run build && bumpp --commit --push --tag && pnpm publish"
  },
  "dependencies": {
    "@unocss/preset-web-fonts": "^65.4.2",
    "@unocss/reset": "65.4.2",
    "ts-cssjson": "^3.0.1",
    "unocss": "^65.4.2",
    "unocss-preset-fluid-sizing": "^0.0.6",
    "unocss-preset-scale-px": "^0.0.1"
  },
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "@types/bun": "latest",
    "fs-extra": "^11.3.0",
    "unbuild": "^3.3.1"
  }
}
