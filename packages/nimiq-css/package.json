{
  "name": "nimiq-css",
  "type": "module",
  "version": "1.0.0-beta.3",
  "description": "Nimiq's Design System in CSS",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./css": {
      "import": "./dist/css/*"
    },
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "release": "pnpm run build && bumpp --commit --push --tag && pnpm publish"
  },
  "dependencies": {
    "@unocss/preset-web-fonts": "catalog:",
    "@unocss/reset": "catalog:",
    "ts-cssjson": "^3.0.1",
    "unocss": "catalog:"
  },
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "@types/bun": "latest",
    "fs-extra": "^11.3.0",
    "unbuild": "^3.3.1"
  }
}
