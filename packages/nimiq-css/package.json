{
  "name": "nimiq-css",
  "type": "module",
  "version": "0.2.27",
  "description": "Nimiq's Design System in CSS",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./css": {
      "import": "./dist/css/*"
    },
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "unbuild && cp -r src/css dist/css/unminified"
  },
  "dependencies": {
    "@unocss/reset": "0.63.2",
    "ts-cssjson": "^3.0.1",
    "unocss": "^0.63.2"
  },
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "@types/bun": "latest",
    "@unocss/preset-web-fonts": "^0.63.3",
    "unbuild": "^2.0.0"
  }
}
