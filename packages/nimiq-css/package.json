{
  "name": "nimiq-css",
  "type": "module",
  "version": "1.0.0-beta.135",
  "description": "Nimiq's Design System in CSS",
  "sideEffects": false,
  "exports": {
    "./css/animations.css": "./dist/css/animations.css",
    "./css/atomic.css": "./dist/css/atomic.css",
    "./css/colors.css": "./dist/css/colors.css",
    "./css/fonts.css": "./dist/css/fonts.css",
    "./css/index.css": "./dist/css/index.css",
    "./css/preflight.css": "./dist/css/preflight.css",
    "./css/spacing.css": "./dist/css/spacing.css",
    "./css/static-content.css": "./dist/css/static-content.css",
    "./css/typography.css": "./dist/css/typography.css",
    "./css/utilities.css": "./dist/css/utilities.css",
    "./css/legacy/icons.css": "./dist/css/legacy/icons.css",
    "./css/legacy/inputs.css": "./dist/css/legacy/inputs.css",
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@iconify/utils": "catalog:icons",
    "@unocss/preset-web-fonts": "catalog:style",
    "@unocss/preset-wind4": "catalog:style",
    "nimiq-icons": "workspace:*",
    "ts-cssjson": "catalog:prod",
    "unocss": "catalog:style",
    "unocss-preset-light-dark": "catalog:style"
  },
  "devDependencies": {
    "@iconify/tools": "catalog:icons",
    "@iconify/types": "catalog:icons",
    "pathe": "catalog:node",
    "unbuild": "catalog:build",
    "vitest": "catalog:test"
  }
}
